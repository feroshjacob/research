<plugin>
   <extension
         point="org.eclipse.ui.views">
      <category
            name="CSeR"
            id="CSeR">
      </category>
      <view
            name="Templates"
            icon="icons/sample.gif"
            category="CSeR"
            class="cser.ast.view.CSeRCloneView"
            id="cser.ui.views.RCFView"/>
   </extension>

   <extension id ="cserjavaeditor"
         point="org.eclipse.ui.editors">
      <editor
            name="CSeR Java Editor"
            default="true"
            icon="icons/sample.gif"
            contributorClass="org.eclipse.jdt.internal.ui.javaeditor.CompilationUnitEditorActionContributor"
            class="cser.editors.CSeREditor"
            id="cser.editors.RCFEditor">
                 <contentTypeBinding contentTypeId="org.eclipse.jdt.core.javaSource"/>
            
      </editor>
     </extension>
